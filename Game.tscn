[sub_resource type="GDScript" id=42]
script/source = "extends AudioStreamPlayer


export var fan_starting: AudioStream
export var fan_standard: AudioStream
export var fan_ending: AudioStream

var is_running: bool = true

func start_fanning():
	stream = fan_starting
	play()

func end_fanning():
	stream = fan_ending
	play()


func _on_FanAudioStreamPlayer_finished():
	if stream == fan_starting and is_running:
		print(\"Fan started, now going on\")
		stream = fan_standard
		play()
"


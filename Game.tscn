[sub_resource type="GDScript" id=56]
script/source = "extends Sprite


func pop():
	region_enabled = false
	$Timer.start(0.1)

func _on_Timer_timeout():
	region_enabled = true

export var plus_label_font: Font

func show_plus_count(plus: int):
	var label := Label.new()
	var tween := Tween.new()
	
	add_child(label)
	label.add_child(tween)
	
	label.text = \"+%sC\" % plus
	
	label.rect_size = Vector2(800, 800)
	label.rect_position = Vector2(-400, -400)
	
	label.align = Label.ALIGN_CENTER
	label.valign = Label.VALIGN_CENTER
	
	label.set(\"custom_fonts/font\", plus_label_font)
	
	label.mouse_filter = Control.MOUSE_FILTER_IGNORE
	tween.interpolate_property(label, \"rect_position\", Vector2(-400, -400), Vector2(-400, -800), 3)
	tween.interpolate_property(label, \"modulate\", Color(1, 1, 1, 1), Color(1, 1, 1, 0), 3)
	tween.start()

func _on_Mouse_pressed():
	pop()
	
	show_plus_count(GameData.wealth_increase_on_click)

func _on_AutoclickTimer_timeout():
	if GameData.clicks_per_seconds > 0: 
		show_plus_count(GameData.clicks_per_seconds)
"

[sub_resource type="GDScript" id=58]
script/source = "extends Sprite


var is_to_remain_shown: bool = false setget set_is_to_remain_shown
var is_shown: bool = false

func set_is_to_remain_shown(new_is_to_remain_shown):
	if not is_to_remain_shown == new_is_to_remain_shown:
		is_to_remain_shown = new_is_to_remain_shown
		
		if not is_shown and new_is_to_remain_shown and $BlitTimer.is_stopped():
			show()
			is_shown = true
			$BlitTimer.start()


func _on_BlitTimer_timeout():
	if is_shown:
		hide()
		is_shown = false
		$BlitTimer.start()
	
	elif is_to_remain_shown:
		show()
		is_shown = true
		$BlitTimer.start()
"


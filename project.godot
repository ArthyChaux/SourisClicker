; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="SourisClicker"
run/main_scene="res://Game.tscn"
boot_splash/image="res://assets/sprites/Fond01.png"
boot_splash/use_filter=false
config/icon="res://icon.png"
config/windows_native_icon="res://assets/sprites/icons/IconeDuJeu196.ico"
config/quit_on_go_back=false

[autoload]

GameData="*res://scripts/GameData.gd"

[display]

window/size/width=1080
window/size/height=1920
window/size/test_width=650
window/size/test_height=1000
window/handheld/orientation="sensor"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[global]

pixel=false

[gui]

theme/custom="res://assets/fonts/theme_global.tres"
theme/custom_font="res://assets/fonts/dynamicfont_64-global.tres"

[locale]

test="fr"
locale_filter=[ 0, [ "en", "fr", "fr_CH", "it", "ja" ] ]
translations=PoolStringArray( "res://assets/translation/translations.en.translation", "res://assets/translation/translations.fr.translation", "res://assets/translation/translations.fr_CH.translation", "res://assets/translation/translations.ja.translation", "res://assets/translation/translations.it.translation" )

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
